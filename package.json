{
    "name": "@refactorjs/ofetch",
    "version": "0.2.0",
    "description": "An extension to ohmyfetch/ofetch",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/refactorjs/ofetch.git"
    },
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "release": "standard-version && git push --follow-tags && npm publish"
    },
    "dependencies": {
        "ohmyfetch": "^0.4.21",
        "cookie-es": "^0.5.0",
        "destr": "^1.2.0",
        "defu": "^6.1.0"
    },
    "devDependencies": {
        "@types/node": "^18.11.9",
        "standard-version": "^9.5.0",
        "typescript": "^4.8.4",
        "unbuild": "^0.9.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "bugs": {
        "url": "https://github.com/refactorjs/ofetch/issues"
    },
    "homepage": "https://github.com/refactorjs/ofetch#readme",
    "keywords": [
        "http",
        "ohmyfetch",
        "ofetch"
    ]
}
